<FirstWindow>:
    name: "first"

    FloatLayout:
             
        Label:
            text: "Menuplan"
            font_size: 64
            color: 0.203, 0.572, 0.89
            size_hint: 1,.5
            background_color: (1, 1, 1, 1)     
            font_name: "Comic"
            pos_hint: {'center_x': 0.5, 'center_y': 0.8}

    BoxLayout:
        orientation: 'horizontal'
        size: root.width, root.height
        spacing: 2

    
        Button:
            id: ButtonMonday
            text: "Monday"
            font_size: 14
            color: 0.203, 0.572, 0.69
            size_hint: (0.12, 0.05)
            pos_hint: {'y': 0.5}
            background_color: 0.203, 0.572, 1
            font_name: "Comic" 
            on_release: app.root.current = "Monday"
        Button:
            id: ButtonTuesday
            text: "Tuesday"
            font_size: 14
            color: 0.203, 0.572, 0.69
            size_hint: (0.12, 0.05)
            pos_hint: {'y': 0.5}
            background_color: 0.203, 0.572, 1
            font_name: "Comic" 
            on_release: app.root.current = "Tuesday"
        Button:
            id: ButtonWednesday
            text: "Wednesday"
            font_size: 14
            color: 0.203, 0.572, 0.69
            size_hint: (0.12, 0.05)
            pos_hint: {'y': 0.5}
            background_color: 0.203, 0.572, 1
            font_name: "Comic" 
            on_release: app.root.current = "Wednesday"
        Button:
            id: ButtonThursday
            text: "Thursday"
            font_size: 14
            color: 0.203, 0.572, 0.69
            size_hint: (0.12, 0.05)
            pos_hint: {'y': 0.5}
            background_color: 0.203, 0.572, 1
            font_name: "Comic" 
            on_release: app.root.current = "Thursday"
        Button:
            id: ButtonFriday
            text: "Friday"
            font_size: 14
            color: 0.203, 0.572, 0.69
            size_hint: (0.12, 0.05)
            pos_hint: {'y': 0.5}
            background_color: 0.203, 0.572, 1
            font_name: "Comic" 
            on_release: app.root.current = "Friday"
        Button:
            id: ButtonSaturday
            text: "Saturday"
            font_size: 14
            color: 0.203, 0.572, 0.69
            size_hint: (0.12, 0.05)
            pos_hint: {'y': 0.5}
            background_color: 0.203, 0.572, 1
            font_name: "Comic" 
            on_release: app.root.current = "Saturday"
        Button:
            id: ButtonSunday
            text: "Sunday"
            font_size: 14
            color: 0.203, 0.572, 0.69
            size_hint: (0.12, 0.05)
            pos_hint: {'y': 0.5}
            background_color: 0.203, 0.572, 1
            font_name: "Comic" 
            on_release: app.root.current = "Sunday"
       

<MondayWindow>:
